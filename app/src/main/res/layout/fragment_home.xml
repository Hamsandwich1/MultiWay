<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#e4d5b7"
    tools:context=".ui.home.HomeFragment">


    <Button
        android:id="@+id/rounded_button2"
        android:layout_width="150dp"
        android:layout_height="95dp"
        android:layout_marginTop="24dp"
        android:backgroundTint="@color/teal_700"
        android:text="Rounded Button2"
        android:textColor="@android:color/white"
        android:textSize="15dp"
        app:cornerRadius="16dp"
        app:layout_constraintBottom_toTopOf="@+id/rounded_button4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/rounded_button1"
        app:layout_constraintTop_toBottomOf="@+id/rounded_button5"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/rounded_button4"
        android:layout_width="150dp"
        android:layout_height="95dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="8dp"
        android:backgroundTint="@color/teal_700"
        android:text="Rounded Button"
        android:textColor="@android:color/white"
        android:textSize="15dp"
        app:cornerRadius="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/rounded_button3"
        app:layout_constraintTop_toBottomOf="@+id/rounded_button2"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/rounded_button3"
        android:layout_width="150dp"
        android:layout_height="95dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="52dp"
        android:backgroundTint="@color/teal_700"
        android:text="Rounded Button3"
        android:textColor="@android:color/white"
        android:textSize="15dp"
        app:cornerRadius="16dp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/rounded_button4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rounded_button1"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/rounded_button5"
        android:layout_width="374dp"
        android:layout_height="95dp"
        android:backgroundTint="@color/teal_700"
        android:text="Rounded Button1"
        android:textColor="@android:color/white"
        android:textSize="30dp"
        app:cornerRadius="16dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="273dp" />

    <Button
        android:id="@+id/rounded_button1"
        android:layout_width="150dp"
        android:layout_height="95dp"
        android:layout_marginTop="24dp"
        android:backgroundTint="@color/teal_700"
        android:text="Rounded Button1"
        android:textColor="@android:color/white"
        android:textSize="15dp"
        app:cornerRadius="16dp"
        app:layout_constraintBottom_toTopOf="@+id/rounded_button3"
        app:layout_constraintEnd_toStartOf="@+id/rounded_button2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rounded_button5"
        app:layout_constraintVertical_bias="0.5" />

    <SearchView
        android:layout_width="356dp"
        android:layout_height="82dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="481dp"
        android:layout_height="124dp"
        android:text="Welcome to MultiWay!"
        android:textSize="50dp"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="96dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="20dp" />


</androidx.constraintlayout.widget.ConstraintLayout>